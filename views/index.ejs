<style>

#summary {
  position: fixed;
  background: rgba(44, 60, 72, 0.8);
  display: none;
  top: 0px;
  bottom: 0px;
  right: 0px;
  left: 0px;
  z-index: 3;
  text-align: center;
}
#summary h1 {
  color: #C0C5C1;
  text-transform: uppercase;
  font-size: 19pt;
  padding: 50px;
}
#summary ul {
  display: inline;
  list-style-type: none;
  padding-right: 20px;
}

.thumb-container {
  display: inline-block;
  margin: auto;
}

.thumb {
  position: relative;
  width: 160px;
  height: 200px;
  display: inline-block;
  margin: 20px;
}
.thumb img {
    width: 100%;
    margin: 0px;
    padding: 0px;
    border:1px solid #;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    border-radius: 5px;
    margin-bottom: 13px;
}
.thumb p {
    color: #AEB4B9;
    font-size: 12;
    text-transform: uppercase;
    margin: 0;
    text-align: left;
    padding-bottom: 2px;
}
.thumb .viewCount {
  color: #AEB4B9;
}
.thumb .uploaded {
  color: #FFF;
}
.thumb .action {
  color: #ffd039;
}

</style>

<body onbeforeunload=javascript:onExit()>

  <!--VIDEO-->
  <div id="smooth"></div>

  <!--CONTROLS-->
  <div id="controls">
    <div class="control-button" id="power-button" data-toggle="tooltip">
      <div class="icon ss-icon">globe</div>
    </div>
    <div id="button-group">
      <div class="control-button" id="back" data-toggle="tooltip">
        <div class="icon ss-icon">previous</div>
      </div>
      <div class="control-button" id="hold" data-toggle="tooltip">
        <div class="icon ss-icon">stop</div>
      </div>
      <div class="control-button" id="live" data-toggle="tooltip">
        <div class="icon ss-icon">next</div>
      </div>
    </div><!--END button-group-->
    <div id="control-status" class="label">
      <div id="control-status-dot" class="live"></div>
      <div id="control-status-message">Live Stream</div>
    </div>
    <div class="control-button" id="share-video" toggle="tooltip">
      <div class="icon ss-icon">upload</div>
    </div>
  </div><!--END controls-->
  <div id="controls-background"></div>
  <div id="summary">
  </div>
<!--TOOLTIP-->
<div class="tooltip-container"></div>

<script id="summary-template" type="text/x-handlebars-template">
  <h1 id="summary-title">Previously on Astronaut</h1>
  <div class='thumb-container'>
    {{#each videos}}
      <div class="thumb">
        <img src="http://img.youtube.com/vi/{{ id }}/0.jpg">
        <p class="viewCount">{{ viewCount }} views</p>
        <p class="uploaded {{uploaded-class}} ">{{ uploaded }}</p>
        <p class="action">{{ action }}</p>
      </div>
     {{/each}}
  </div>
  <h1 id="summary-caption">Astronaut</h1>
</script>

